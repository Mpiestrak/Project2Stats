<h1>This is where the form goes</h1>

<form method="POST" action="/league" enctype="multipart/form-data">
                    <div class="form-group">
                        <label for="League">League name:</label>
                        <input name="name" type="text" class="form-control" id="leagueInputName" aria-describedby="leagueNameHelp"
                            placeholder="Enter name">
                    </div>
                    <div class="form-group">
                        <label for="bDate">Begin date:</label>
                        <input name="bDate" type="date" class="form-control" id="bDate" placeholder="Date league begins">
                    </div>
                    <div class="form-group">
                        <label for="eDate">End date:</label>
                        <input name="eDate" type="date" class="form-control" id="eDate" placeholder="Date league ends">
                    </div>
                    <div class="form-group">
                        <label for="numberOfTeams">Number of teams in league:</label>
                        <input name="numberOfTeams" type="number" class="form-control" id="numberOfTeams" placeholder="Number of teams in league">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>